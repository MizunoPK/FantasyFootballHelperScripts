{
  "manifest_version": 3,
  "name": "NFL Fantasy Data Exporter",
  "version": "1.0.0",
  "description": "Export player ownership data from NFL Fantasy to CSV",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "https://fantasy.nfl.com/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
